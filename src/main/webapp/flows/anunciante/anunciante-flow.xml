<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd"
        start-state="subirContenido">


      <on-start>
            <evaluate expression="mantenedorContenido.inicio()"/>
      </on-start>
      <view-state id="inicioAnunciante">

      </view-state>

      <view-state id="subirContenido">
            <transition on="sub" to="finish" ></transition>
      </view-state>


      <end-state id="finish" view="/flows/campana/" />

</flow>
