<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd"
      start-state="registraAnuncio">

      <on-start>
            <evaluate expression="mantenedorAnuncios.inicio()"/>
      </on-start>

      <view-state id="registraAnuncio" popup="true">
            <transition on="Registrar" to="listarAnuncios"></transition>
      </view-state>
      <view-state id="listarAnuncios" view="listarAnuncios.xhtml">

      </view-state>


</flow>