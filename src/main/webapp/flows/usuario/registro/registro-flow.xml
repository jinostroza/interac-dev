<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd"
      start-state="registrar">

    <on-start>
        <evaluate expression="mantenedorRegistroUsuario.inicio()"/>
    </on-start>

    <view-state id="registrar">
        <transition on="flowPaso2" to="registrar_paso2"/>
    </view-state>

    <view-state id="registrar_paso2">
        <transition on="flowPaso1" to="registrar"/>
    </view-state>
</flow>