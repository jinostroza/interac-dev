<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd"
      start-state="campanapasos">
      <on-start>
            <evaluate expression="mantenedorContenido.inicio()"/>
      </on-start>
      <view-state id="campanapasos">
            <transition on="reg" to="registraCampana">

            </transition>
            <transition on="next2" to="seleccionarTotem"></transition>
      </view-state>


      <view-state id="registraCampana" >

      </view-state>
      <view-state id="seleccionarTotem">



      </view-state>

</flow>