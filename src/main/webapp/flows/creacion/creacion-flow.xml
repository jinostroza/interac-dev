<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd"
      start-state="registraAnuncio">

    <on-start>
        <evaluate expression="mantenedorAnuncios.inicio()"/>
    </on-start>

    <view-state id="registraAnuncio">
        <transition on="reg" to="registraCampana">

        </transition>

    </view-state>


   <view-state id="seleccionarTotem">
       <transition on="finalizar" to="finalizar" />
    </view-state>
    <view-state id="crearcampana">
        <transition on="con" to="registraAnuncio" />
    </view-state>
    <view-state id="finalizar">

    </view-state>
</flow>