doctype html
html
    head
        title
        link(href='public/css/style.css', rel='stylesheet')
        link(rel='stylesheet', type='text/css', href='public/css/clock.css')
        script(src='public/js/jquery-2.1.4.min.js')
        script(src='public/js/slide.js')
        script(src='public/js/dashbox.js')
        script(src='public/js/twitterFetcher_min.js')
       

    body

        iframe.overlay(style='display:none')
        .container
            .header
                #weather.weather
                      a(href='//booked.getElementsByTagName/weather/valparaiso-w632826')
                      img(src='//w.bookcdn.com/weather/picture/30_w632826_1_4_34495e_250_2c3e50_ffffff_ffffff_1_2071c9_ffffff_0_3.png?scode=124&domid=w209')
                #historia
                #pucv
                #interac.facebook
                #twitter-logo.twitter
                #id-twitter-pucv.twitter@iHistoriaPucv        
            #content.content(style='padding: 0px')
div(class="footer")
                #twitter.twitter(href='http://twitter.com/ihistoriapucv') Cargando Tweets
            div(class="dashbox")
                button(class="flickrhistoria" id="flickrhistoria")
                button(class="historiabtn" id="historiabtn")
                button(class="pucvbtn" id="pucvbtn")

        script(type='text/javascript').
            jQuery(document).ready(function () {
            });
            var LatestTweets = {
                init: function () {
                    twitterFetcher.fetch({
                        id: '712628953064849409',
                        domId: 'twitter',
                        maxTweets: 5
                    });
                    // analytics
             (function (i, s, o, g, r, a, m) {
              i['GoogleAnalyticsObject'] = r;
              i[r] = i[r] || function () {
              (i[r].q = i[r].q || []).push(arguments)
              }, i[r].l = 1 * new Date();
               a = s.createElement(o),
                  m = s.getElementsByTagName(o)[0];
                  a.async = 1;
                  a.src = g;
                m.parentNode.insertBefore(a, m)
                  })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');
                ga('create', 'UA-69484091-4', {'cookiedomain': 'none'});
                ga('send', 'pageview');
                 // custom
                 $('#flickrhistoria').on('click', function () {
                 ga('send', 'event', 'btnflickrhistoria', 'click', 'nav-buttons');
                 });
                 $('#historiabtn').on('click', function () {
                  ga('send', 'event', 'btnhistoria', 'click', 'nav-buttons');
                 });
                 $('#pucvbtn').on('click', function () {
                  ga('send', 'event', 'btnpucv', 'click', 'nav-buttons');
                 });
                 $('#twitter').on('click', function () {
                  ga('send', 'event', 'divtwitter', 'click', 'nav-buttons');
                 });
                 $('#facebook').on('click', function () {
                  ga('send', 'event', 'divfacebook', 'click', 'nav-buttons');
                 });
                 $('#weather').on('click', function () {
                  ga('send', 'event', 'divweather', 'click', 'nav-buttons');
                 });
                }
            }
            Slide.start();
            DashBox.start();
            LatestTweets.init();
