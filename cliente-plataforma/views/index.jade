                button#btn2.academicosUsach
                  div.textos Acádemicos
                button#btn3.investigacionUsach
                  div.textos Ingeniería


        script(type='text/javascript').
            jQuery(document).ready(function () {
            });
            var LatestTweets = {
                init: function () {
